INSTALL = @INSTALL@

prefix = @prefix@

sources = $(wildcard *.py)

.PHONY: all install install-lib clean distclean print

all: 

install: $(sources)
	$(INSTALL) -d $(prefix)/bin
	$(INSTALL) -v -m 644 $(sources) $(prefix)/bin

install-lib:

clean:

distclean: clean
	rm -f Makefile
